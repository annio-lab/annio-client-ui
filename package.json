{
  "name": "annio-ui",
  "displayName": "Annio App",
  "version": "1.0.0",
  "icon": "https://opensume.com/favicon.ico",
  "description": "Annio App - Microservice Shop",
  "author": {
    "name": "Bill Tran",
    "email": "tvkhue96@gmail.com"
  },
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "dev:pm2": "npm run pm2:stop && ./node_modules/.bin/pm2 startOrReload ./ecosystem.yml && npm run logs",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "./node_modules/.bin/pm2-runtime ./ecosystem-prod.yml",
    "test": "jest --coverage --passWithNoTests",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export -o build",
    "logs": "./node_modules/.bin/pm2 logs",
    "logs:flush": "./node_modules/.bin/pm2 flush",
    "pm2:stop": "./node_modules/.bin/pm2 kill",
    "pm2:dashboard": "./node_modules/.bin/pm2 plus",
    "pm2:process": "./node_modules/.bin/pm2 monit",
    "pm2:upgrade": "./node_modules/.bin/pm2 update",
    "upgrade": "npx run yarn-upgrade-all"
  },
  "dependencies": {
    "@annio/core": "1.0.5",
    "@ckeditor/ckeditor5-build-classic": "^29.1.0",
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@next/bundle-analyzer": "11.0.1",
    "@reduxjs/toolkit": "1.6.0",
    "autoprefixer": "10.2.6",
    "axios": "0.21.1",
    "bootstrap": "4.5.2",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "date-fns": "2.22.1",
    "detect-node": "2.1.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "i18next": "20.3.2",
    "i18next-chained-backend": "3.0.2",
    "i18next-fs-backend": "1.1.1",
    "i18next-http-backend": "1.2.6",
    "i18next-http-middleware": "3.1.4",
    "isomorphic-unfetch": "3.1.0",
    "js-cookie": "2.2.1",
    "less": "4.1.1",
    "localforage": "1.9.0",
    "lodash": "4.17.21",
    "next": "11.0.1",
    "next-compose-plugins": "2.2.1",
    "next-routes": "1.4.2",
    "nodemon": "2.0.11",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-bootstrap-table-next": "4.0.3",
    "react-dom": "17.0.2",
    "react-hammerjs": "^1.0.1",
    "react-hook-form": "6.10.1",
    "react-i18next": "11.11.1",
    "react-markdown": "^6.0.3",
    "react-redux": "7.2.4",
    "reactstrap": "8.9.0",
    "redux-observable": "1.2.0",
    "redux-persist": "6.0.0",
    "rxjs": "6.6.3",
    "sass": "1.26.11",
    "styled-components": "5.3.0",
    "sweetalert": "2.1.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/classnames": "2.3.1",
    "@types/detect-node": "2.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.12",
    "@types/i18next-fs-backend": "1.0.1",
    "@types/jest": "26.0.23",
    "@types/js-cookie": "2.2.6",
    "@types/lodash": "4.14.170",
    "@types/node": "16.0.0",
    "@types/react": "17.0.13",
    "@types/react-bootstrap-table-next": "4.0.14",
    "@types/react-hammerjs": "^1.0.2",
    "@types/react-redux": "7.1.16",
    "@types/styled-components": "5.1.11",
    "@types/testing-library__react": "10.2.0",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.30.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-styled-components": "1.13.1",
    "eslint": "7.30.0",
